    "clean-css": "^2.2.19",
    "cloudflare-cli": "^3.2.1",
    "configury": "^0.2.0",
    "front-matter": "^0.2.0",
    "fs-extra": "^0.12.0",
    "glob": "^7.1.3",
    "html-minifier": "^3.5.21",
    "jade": "^1.8.1",
    "jstransformer-uglify-js": "^1.2.0",
    "lodash.merge": "^2.4.1",
    "mkdirp": "^0.5.0",
