{
  "name": "benedfit.com",
  "version": "3.0.0",
  "description": "Personal website of Ben Edwards, a Web Designer and Developer",
  "private": true,
  "scripts": {
    "start": "pliers go -o",
    "deploy": "bash ./deploy.sh",
    "lintCss": "./node_modules/.bin/csslint .",
    "lintPug": "pug-lint .",
    "lintJs": "./node_modules/.bin/jshint",
    "checkStyle": "./node_modules/.bin/jscs .",
    "wrap": "npm prune && npm shrinkwrap && pliers cleanShrinkwrap",
    "pretest": "pliers build && npm run lintCss && npm run lintJs && npm run checkStyle && npm run lintPug",
    "test": "./node_modules/.bin/istanbul --include-all-sources cover ./node_modules/.bin/_mocha -- --recursive -R spec",
    "posttest": "./node_modules/.bin/istanbul check-coverage && rm -rf coverage",
    "postinstall": "pliers build",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "any-newer-files": "0.0.4",
    "async": "^0.9.0",
    "autoprefixer-stylus": "^0.4.0",
    "browser-sync": "^2.3.1",
    "clean-css": "^2.2.19",
    "cloudflare-cli": "0.0.10",
    "configury": "^0.2.0",
    "front-matter": "^0.2.0",
    "fs-extra": "^0.12.0",
    "glob": "^4.0.6",
    "html-minifier": "^0.6.9",
    "jade": "^1.8.1",
    "lodash.merge": "^2.4.1",
    "mkdirp": "^0.5.0",
    "moment": "^2.8.4",
    "namp": "^0.2.25",
    "rfg-api": "^0.1.7",
    "rimraf": "^2.2.8",
    "stylus": "^0.49.2",
    "stylus-mixins": "^0.1.6",
    "stylus-renderer": "^1.1.0",
    "snyk": "^1.17.1"
  },
  "devDependencies": {
    "csslint": "^0.10.0",
    "istanbul": "^0.3.10",
    "jscs": "^1.8.1",
    "jshint": "^2.5.10",
    "mocha": "^2.2.5",
    "pliers": "^1.1.2",
    "pliers-clean-shrinkwrap": "^1.0.1",
    "pliers-imagemin": "^2.0.2",
    "pliers-npm-security-check": "^1.0.0",
    "pug-lint": "^2.2.2",
    "pug-lint-config-clock": "^2.0.0"
  },
  "snyk": true
}