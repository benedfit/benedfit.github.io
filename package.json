{
  "name": "benedfit.com",
  "version": "3.0.0",
  "description": "Personal website of Ben Edwards, a Web Designer and Developer",
  "private": true,
  "engines": {
    "node": "^14.0.0",
    "npm": "^7.0.0"
  },
  "scripts": {
    "start": "pliers go -o",
    "build": "pliers build",
    "lintCss": "./node_modules/.bin/csslint .",
    "lintPug": "pug-lint .",
    "lintJs": "./node_modules/.bin/jshint",
    "checkStyle": "./node_modules/.bin/jscs .",
    "pretest": "pliers build && npm run lintCss && npm run lintJs && npm run lintPug",
    "test": "./node_modules/.bin/istanbul --include-all-sources cover ./node_modules/.bin/_mocha -- --recursive -R spec",
    "posttest": "./node_modules/.bin/istanbul check-coverage && rm -rf coverage"
  },
  "dependencies": {
    "any-newer-files": "0.0.4",
    "async": "3.2.0",
    "autoprefixer-stylus": "1.0.0",
    "browser-sync": "2.26.14",
    "clean-css": "5.1.2",
    "cloudflare-cli": "4.2.0",
    "configury": "0.3.0",
    "front-matter": "4.0.2",
    "fs-extra": "10.0.0",
    "glob": "7.1.7",
    "html-minifier": "4.0.0",
    "jade": "1.11.0",
    "jstransformer-uglify-js": "1.2.0",
    "lodash.merge": "4.6.2",
    "mkdirp": "1.0.4",
    "moment": "2.29.1",
    "namp": "0.2.26",
    "rfg-api": "0.5.2",
    "rimraf": "3.0.2",
    "stylus": "0.54.8",
    "stylus-mixins": "0.4.0",
    "stylus-renderer": "1.1.0"
  },
  "devDependencies": {
    "csslint": "1.0.5",
    "istanbul": "0.4.5",
    "jscs": "3.0.7",
    "jshint": "2.12.0",
    "mocha": "8.4.0",
    "pliers": "1.2.1",
    "pliers-clean-shrinkwrap": "1.0.1",
    "pliers-imagemin": "3.0.2",
    "pliers-npm-security-check": "1.0.0",
    "pug-lint": "2.6.0",
    "pug-lint-config-clock": "2.0.0"
  }
}
